{
    "name": "0100dev/cakephp-rabbitmq",
    "description": "A RabbitMQ plugin for CakePHP 4",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "0100Dev",
        "rabbitmq",
        "consumers",
        "producers"
    ],
    "homepage": "https://github.com/umer936/cakephp-rabbitmq",
    "license": "MIT",
    "authors": [
        {
            "name": "Wouter van Os",
            "homepage": "https://0100dev.nl",
            "role": "Author"
        },
        {
            "name": "Marlin Cremers",
            "homepage": "https://cvo-technologies.com/",
            "role": "Author"
        }
    ],
    "support": {
        "issues": "https://github.com/0100Dev/cakephp-rabbitmq/issues",
        "source": "https://github.com/0100Dev/cakephp-rabbitmq"
    },
    "require": {
        "php": ">=7.4",
	    "php-amqplib/php-amqplib": "^3.0.0",
        "cakephp/cakephp": "^4.0.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "cakephp/debug_kit": "^4.0",
        "cakephp/cakephp-codesniffer": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
            "DevApp\\RabbitMQ\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "tests/TestApp/src",
            "DevApp\\RabbitMQ\\Test\\": "tests"
        }
    }
}
